    <script>
        // Funkcjonalność edytora demo (przeciąganie i paleta kolorów) - istniejąca
        const draggableElement = document.getElementById('draggable-element');
        const canvas = document.getElementById('canvas');
        
        let isDragging = false;
        let offsetX, offsetY;
        
        if (draggableElement && canvas) { // Sprawdź, czy elementy istnieją
            draggableElement.addEventListener('mousedown', (e) => {
                isDragging = true;
                offsetX = e.clientX - draggableElement.getBoundingClientRect().left;
                offsetY = e.clientY - draggableElement.getBoundingClientRect().top;
            });
            
            document.addEventListener('mousemove', (e) => {
                if (!isDragging) return;
                
                const canvasRect = canvas.getBoundingClientRect();
                let newX = e.clientX - canvasRect.left - offsetX;
                let newY = e.clientY - canvasRect.top - offsetY;
                
                // Ogranicz element do granic płótna
                newX = Math.max(0, Math.min(newX, canvasRect.width - draggableElement.offsetWidth));
                newY = Math.max(0, Math.min(newY, canvasRect.height - draggableElement.offsetHeight));
                
                draggableElement.style.left = newX + 'px';
                draggableElement.style.top = newY + 'px';
            });
            
            document.addEventListener('mouseup', () => {
                isDragging = false;
            });
            
            // Funkcjonalność palety kolorów
            const colorPalettes = document.querySelectorAll('.color-palette');
            colorPalettes.forEach(palette => {
                palette.addEventListener('click', () => {
                    const color = window.getComputedStyle(palette).backgroundColor;
                    draggableElement.style.backgroundColor = color;
                });
            });
        }

        // Nowa funkcjonalność demo animacji z melody.html
        const presentationContainer = document.querySelector('.presentation-container');
        const slideThumbnails = document.querySelectorAll('.slide-thumbnail');
        const slideContent = document.querySelector('.slide-content');
        const elementLibrary = document.querySelector('.element-library');
        const templatePreview = document.querySelector('.template-preview');
        const finalPresentation = document.querySelector('.final-presentation');
        const cursor = document.querySelector('.cursor');
        const progressBar = document.querySelector('.progress-bar');
        const restartButton = document.getElementById('restart-animation');
        const skipButton = document.getElementById('skip-animation');

        let animationTimeout;
        let currentStep = 0;
        const steps = [
            // Step 0: Initial state (already set by HTML/CSS)
            () => {
                // Initial state, ensure elements are reset
                slideThumbnails.forEach(thumb => thumb.classList.remove('active'));
                slideThumbnails[0].classList.add('active');
                slideContent.classList.add('active');
                slideContent.querySelectorAll('.slide-title, .slide-text, .slide-image').forEach(el => {
                    el.style.transform = 'translateY(20px)';
                    el.style.opacity = '0';
                });
                elementLibrary.classList.remove('active');
                templatePreview.classList.remove('active');
                finalPresentation.classList.remove('active');
                progressBar.style.width = '0%';
                cursor.style.transform = 'translate(50px, 50px)';
                cursor.classList.remove('animate-cursor-1', 'animate-cursor-2', 'animate-cursor-3', 'animate-cursor-4', 'animate-cursor-5', 'pulse');
            },
            // Step 1: Cursor moves to add element button
            () => {
                cursor.classList.add('animate-cursor-1');
                cursor.style.transform = 'translate(200px, 100px)'; // Example target for "add element" button
                setTimeout(() => cursor.classList.add('pulse'), 1800); // Pulse at the end of movement
            },
            // Step 2: Element library appears, cursor clicks "Image"
            () => {
                elementLibrary.classList.add('active');
                cursor.classList.remove('animate-cursor-1', 'pulse');
                cursor.classList.add('animate-cursor-2');
                cursor.style.transform = 'translate(250px, 180px)'; // Example target for "Image" in library
                setTimeout(() => cursor.classList.add('pulse'), 1800);
            },
            // Step 3: Image element appears on slide, cursor moves to templates
            () => {
                slideContent.querySelector('.slide-image').style.transform = 'translateY(0)';
                slideContent.querySelector('.slide-image').style.opacity = '1';
                elementLibrary.classList.remove('active');
                cursor.classList.remove('animate-cursor-2', 'pulse');
                cursor.classList.add('animate-cursor-3');
                cursor.style.transform = 'translate(300px, 70px)'; // Example target for "Templates" button
                setTimeout(() => cursor.classList.add('pulse'), 1800);
            },
            // Step 4: Template preview appears, cursor clicks a template
            () => {
                templatePreview.classList.add('active');
                cursor.classList.remove('animate-cursor-3', 'pulse');
                cursor.classList.add('animate-cursor-4');
                cursor.style.transform = 'translate(100px, 200px)'; // Example target for a template item
                setTimeout(() => cursor.classList.add('pulse'), 1800);
            },
            // Step 5: Final presentation appears
            () => {
                templatePreview.classList.remove('active');
                finalPresentation.classList.add('active');
                cursor.classList.remove('animate-cursor-4', 'pulse');
                cursor.classList.add('animate-cursor-5');
                cursor.style.transform = 'translate(400px, 200px)'; // Center of final presentation
                setTimeout(() => cursor.classList.add('pulse'), 1800);
            }
        ];

        function runAnimationStep() {
            if (currentStep < steps.length) {
                steps[currentStep]();
                progressBar.style.width = ((currentStep + 1) / steps.length) * 100 + '%';
                currentStep++;
                animationTimeout = setTimeout(runAnimationStep, 2500); // Adjust timing as needed
            } else {
                // Animation finished, loop or stop
                setTimeout(resetAnimation, 2000); // Reset after a short delay
            }
        }

        function resetAnimation() {
            clearTimeout(animationTimeout);
            currentStep = 0;
            steps[0](); // Reset to initial state
            setTimeout(runAnimationStep, 1000); // Start again after a short delay
        }

        function skipAnimation() {
            clearTimeout(animationTimeout);
            currentStep = steps.length; // Skip to the end
            progressBar.style.width = '100%';
            finalPresentation.classList.add('active'); // Ensure final state is shown
            elementLibrary.classList.remove('active');
            templatePreview.classList.remove('active');
            slideContent.classList.remove('active'); // Hide editor interface elements
            cursor.style.display = 'none'; // Hide cursor
            // Optionally, show a static image or message instead of the animation
        }

        // Event Listeners for controls
        if (restartButton) {
            restartButton.addEventListener('click', resetAnimation);
        }
        if (skipButton) {
            skipButton.addEventListener('click', skipAnimation);
        }

        // Start animation on window load
        window.onload = function () {
            if (presentationContainer) { // Only run if the demo section exists
                resetAnimation(); // Initial setup and start
            }
        };
    </script>
</body>
</html>
